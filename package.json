{
  "name": "horus.dev",
  "version": "0.0.1",
  "private": true,
  "author": "Horus Lugo <horusgoul@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc",
    "lint:base": "eslint --ext .js,.json,.ts,.tsx",
    "lint": "yarn lint:base 'src/**'"
  },
  "lint-staged": {
    "src/**/*.{js,json,ts,tsx}": [
      "yarn lint:base --fix"
    ],
    "src/**/*.{scss,css}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn typecheck && lint-staged"
    }
  },
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "next": "9.4.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-icons": "^3.10.0",
    "reading-time": "^1.2.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/node": "^14.0.6",
    "@types/react": "^16.9.35",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.7",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.4.6",
    "typescript": "^3.9.3"
  }
}
