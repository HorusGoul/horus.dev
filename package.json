  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc",
    "lint:base": "eslint --ext .js,.json,.ts,.tsx",
    "lint": "yarn lint:base 'src/**'"
  },
  "lint-staged": {
    "src/**/*.{js,json,ts,tsx}": [
      "yarn lint:base --fix"
    ],
    "src/**/*.{scss,css}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn typecheck && lint-staged"
    }
  },
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "body-scroll-lock": "^3.1.3",
    "classnames": "^2.2.6",
    "framer-motion": "^2.6.13",
    "next": "9.5.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-icons": "^3.11.0",
    "react-intersection-observer": "^8.28.5",
    "reading-time": "^1.2.0"
  },
  "devDependencies": {
    "@types/body-scroll-lock": "^2.6.1",
    "@types/classnames": "^2.2.10",
    "@types/node": "^14.10.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.1",
    "sass": "^1.26.10",
    "tailwindcss": "^1.8.9",
    "typescript": "^4.0.2"
  }
}
